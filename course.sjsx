/* Team Section */
/* TeamSection.css */

.team-section {
  padding: 100px 0;
  background: linear-gradient(135deg, #0a1929 0%, #1a2b3c 100%);
  position: relative;
  overflow: hidden;
}

/* Decorative elements */
.team-section::before {
  content: '';
  position: absolute;
  top: -100px;
  right: -100px;
  width: 500px;
  height: 500px;
  background: radial-gradient(circle, rgba(249, 115, 22, 0.15) 0%, transparent 70%);
  border-radius: 50%;
  animation: floatOrb 20s ease-in-out infinite;
}

.team-section::after {
  content: '';
  position: absolute;
  bottom: -100px;
  left: -100px;
  width: 600px;
  height: 600px;
  background: radial-gradient(circle, rgba(249, 115, 22, 0.1) 0%, transparent 70%);
  border-radius: 50%;
  animation: floatOrb 20s ease-in-out infinite reverse;
}

.container {
  max-width: 1400px;
  margin: 0 auto;
  padding: 0 24px;
  position: relative;
  z-index: 2;
}

/* Section Header */
.section-header {
  text-align: center;
  margin-bottom: 70px;
  position: relative;
}

.section-badge {
  display: inline-flex;
  align-items: center;
  gap: 10px;
  background: rgba(249, 115, 22, 0.2);
  color: #f97316;
  font-weight: 600;
  font-size: 14px;
  padding: 10px 28px;
  border-radius: 50px;
  margin-bottom: 20px;
  text-transform: uppercase;
  letter-spacing: 1px;
  border: 1px solid rgba(249, 115, 22, 0.3);
  backdrop-filter: blur(10px);
  animation: fadeInUp 0.6s ease;
}

.section-badge svg {
  width: 18px;
  height: 18px;
}

.section-title {
  font-size: clamp(36px, 5vw, 48px);
  font-weight: 800;
  color: white;
  margin-bottom: 20px;
  animation: fadeInUp 0.6s ease 0.1s both;
  line-height: 1.2;
}

.section-title .highlight {
  color: #f97316;
  position: relative;
  display: inline-block;
}

.section-title .highlight::after {
  content: '';
  position: absolute;
  bottom: 5px;
  left: 0;
  width: 100%;
  height: 8px;
  background: rgba(249, 115, 22, 0.3);
  border-radius: 4px;
  z-index: -1;
}

.section-subtitle {
  font-size: 18px;
  color: rgba(255, 255, 255, 0.7);
  max-width: 600px;
  margin: 0 auto;
  animation: fadeInUp 0.6s ease 0.2s both;
  line-height: 1.6;
}

/* Team Grid */
.team-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 30px;
  margin-top: 20px;
}

/* Team Card */
.team-card {
  background: rgba(255, 255, 255, 0.03);
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.05);
  border-radius: 30px;
  overflow: hidden;
  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  position: relative;
  animation: fadeInUp 0.6s ease forwards;
  opacity: 0;
}

.team-card:nth-child(1) { animation-delay: 0.2s; }
.team-card:nth-child(2) { animation-delay: 0.3s; }
.team-card:nth-child(3) { animation-delay: 0.4s; }
.team-card:nth-child(4) { animation-delay: 0.5s; }

.team-card:hover {
  transform: translateY(-15px) scale(1.02);
  border-color: rgba(249, 115, 22, 0.3);
  box-shadow: 0 30px 60px -20px rgba(249, 115, 22, 0.4);
  background: rgba(255, 255, 255, 0.05);
}

/* Card Image */
.card-image-wrapper {
  position: relative;
  height: 300px;
  overflow: hidden;
}

.team-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.6s ease;
  filter: grayscale(20%);
}

.team-card:hover .team-image {
  transform: scale(1.1);
  filter: grayscale(0%);
}

.image-overlay {
  position: absolute;
  inset: 0;
  background: linear-gradient(180deg, 
    rgba(10, 25, 41, 0.2) 0%,
    rgba(249, 115, 22, 0.3) 100%
  );
  mix-blend-mode: overlay;
}

/* Role Badge */
.role-badge {
  position: absolute;
  top: 20px;
  right: 20px;
  background: rgba(249, 115, 22, 0.9);
  backdrop-filter: blur(5px);
  color: white;
  padding: 8px 20px;
  border-radius: 50px;
  font-size: 13px;
  font-weight: 600;
  letter-spacing: 0.5px;
  z-index: 2;
  box-shadow: 0 5px 15px rgba(249, 115, 22, 0.3);
  border: 1px solid rgba(255, 255, 255, 0.2);
  transition: all 0.3s ease;
}

.team-card:hover .role-badge {
  background: white;
  color: #f97316;
  transform: scale(1.05);
}

/* Social Links */
.social-links {
  position: absolute;
  bottom: 20px;
  left: 0;
  right: 0;
  display: flex;
  justify-content: center;
  gap: 12px;
  z-index: 2;
  opacity: 0;
  transform: translateY(20px);
  transition: all 0.3s ease;
}

.team-card:hover .social-links {
  opacity: 1;
  transform: translateY(0);
}

.social-link {
  width: 40px;
  height: 40px;
  background: rgba(255, 255, 255, 0.9);
  backdrop-filter: blur(5px);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #0a1929;
  transition: all 0.3s ease;
  border: 1px solid rgba(249, 115, 22, 0.3);
}

.social-link:hover {
  background: #f97316;
  color: white;
  transform: translateY(-5px);
  box-shadow: 0 5px 15px rgba(249, 115, 22, 0.4);
}

.social-link svg {
  width: 18px;
  height: 18px;
}

/* Card Content */
.card-content {
  padding: 25px;
  text-align: center;
  position: relative;
}

.member-name {
  font-size: 22px;
  font-weight: 700;
  color: white;
  margin-bottom: 8px;
  transition: color 0.3s ease;
  letter-spacing: -0.5px;
}

.team-card:hover .member-name {
  color: #f97316;
}

.member-title {
  font-size: 15px;
  color: #f97316;
  font-weight: 600;
  margin-bottom: 15px;
  padding: 5px 15px;
  background: rgba(249, 115, 22, 0.1);
  display: inline-block;
  border-radius: 50px;
  border: 1px solid rgba(249, 115, 22, 0.2);
}

.member-bio {
  font-size: 14px;
  line-height: 1.7;
  color: rgba(255, 255, 255, 0.6);
  margin-bottom: 20px;
  transition: color 0.3s ease;
}

.team-card:hover .member-bio {
  color: rgba(255, 255, 255, 0.8);
}

/* Expertise Tags */
.expertise-tags {
  display: flex;
  flex-wrap: wrap;
  gap: 8px;
  justify-content: center;
  margin: 15px 0;
}

.expertise-tag {
  background: rgba(255, 255, 255, 0.03);
  border: 1px solid rgba(255, 255, 255, 0.1);
  color: rgba(255, 255, 255, 0.6);
  padding: 4px 12px;
  border-radius: 50px;
  font-size: 11px;
  font-weight: 500;
  transition: all 0.3s ease;
}

.team-card:hover .expertise-tag {
  background: rgba(249, 115, 22, 0.1);
  border-color: rgba(249, 115, 22, 0.3);
  color: #f97316;
}

/* Experience Badge */
.experience-badge {
  display: inline-flex;
  align-items: center;
  gap: 5px;
  background: rgba(255, 255, 255, 0.03);
  border: 1px solid rgba(255, 255, 255, 0.1);
  color: rgba(255, 255, 255, 0.6);
  padding: 5px 15px;
  border-radius: 50px;
  font-size: 12px;
  font-weight: 500;
  margin-top: 10px;
}

.experience-badge svg {
  width: 14px;
  height: 14px;
  color: #f97316;
}

/* Team Stats */
.team-stats {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 30px;
  margin-top: 70px;
  padding: 40px;
  background: rgba(255, 255, 255, 0.02);
  border-radius: 40px;
  border: 1px solid rgba(255, 255, 255, 0.05);
  backdrop-filter: blur(10px);
  animation: fadeInUp 0.6s ease 0.6s both;
}

.stat-item {
  text-align: center;
  position: relative;
}

.stat-item:not(:last-child)::after {
  content: '';
  position: absolute;
  top: 50%;
  right: -15px;
  transform: translateY(-50%);
  width: 1px;
  height: 40px;
  background: rgba(255, 255, 255, 0.1);
}

.stat-value {
  font-size: 36px;
  font-weight: 800;
  color: #f97316;
  margin-bottom: 5px;
  line-height: 1;
}

.stat-label {
  font-size: 14px;
  color: rgba(255, 255, 255, 0.7);
  text-transform: uppercase;
  letter-spacing: 1px;
  margin-bottom: 5px;
}

.stat-desc {
  font-size: 12px;
  color: rgba(255, 255, 255, 0.4);
}

/* Animations */
@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes floatOrb {
  0%, 100% {
    transform: translate(0, 0) scale(1);
  }
  50% {
    transform: translate(30px, -30px) scale(1.1);
  }
}

/* Responsive Design */
@media (max-width: 1200px) {
  .team-grid {
    grid-template-columns: repeat(2, 1fr);
    gap: 25px;
  }

  .team-stats {
