  font-size: 20px;
  color: rgba(255, 255, 255, 0.8);
  max-width: 700px;
  margin: 0 auto 40px;
  line-height: 1.6;
  animation: fadeInUp 0.6s ease 0.3s both;
}

/* Stats Preview */
