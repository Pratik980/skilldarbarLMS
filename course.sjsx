  /* Course  Section*/
/* PopularCourses.css */

.popular-courses {
  padding: 100px 0;
  background: linear-gradient(135deg, #f8fafc 0%, #f1f5f9 100%);
  position: relative;
  overflow: hidden;
}

/* Decorative elements */
.popular-courses::before {
  content: '';
  position: absolute;
  top: -100px;
  right: -100px;
  width: 400px;
  height: 400px;
  background: radial-gradient(circle, rgba(249, 115, 22, 0.05) 0%, transparent 70%);
  border-radius: 50%;
  z-index: 0;
}

.popular-courses::after {
  content: '';
  position: absolute;
  bottom: -100px;
  left: -100px;
  width: 500px;
  height: 500px;
  background: radial-gradient(circle, rgba(249, 115, 22, 0.03) 0%, transparent 70%);
  border-radius: 50%;
  z-index: 0;
}

.container {
  max-width: 1400px;
  margin: 0 auto;
  padding: 0 24px;
  position: relative;
  z-index: 2;
}

/* Section Header */
.section-header {
  text-align: center;
  margin-bottom: 60px;
  position: relative;
}

.section-badge {
  display: inline-block;
  background: linear-gradient(135deg, #f97316 0%, #ea580c 100%);
  color: white;
  font-weight: 600;
  font-size: 14px;
  padding: 8px 24px;
  border-radius: 50px;
  margin-bottom: 20px;
  text-transform: uppercase;
  letter-spacing: 1px;
  box-shadow: 0 5px 15px rgba(249, 115, 22, 0.3);
  animation: fadeInUp 0.6s ease;
}

.section-title {
  font-size: clamp(36px, 5vw, 48px);
  font-weight: 800;
  color: #0a1929;
  margin-bottom: 20px;
  animation: fadeInUp 0.6s ease 0.1s both;
  line-height: 1.2;
}

.section-title .highlight {
  color: #f97316;
  position: relative;
  display: inline-block;
}

.section-title .highlight::after {
  content: '';
  position: absolute;
  bottom: 5px;
  left: 0;
  width: 100%;
  height: 8px;
  background: rgba(249, 115, 22, 0.2);
  border-radius: 4px;
  z-index: -1;
}

.section-subtitle {
  font-size: 18px;
  color: #64748b;
  max-width: 600px;
  margin: 0 auto;
  animation: fadeInUp 0.6s ease 0.2s both;
  line-height: 1.6;
}

/* Category Tabs */
.category-tabs {
  display: flex;
  justify-content: center;
  gap: 15px;
  margin-bottom: 50px;
  flex-wrap: wrap;
  animation: fadeInUp 0.6s ease 0.3s both;
}

.category-tab {
  padding: 12px 28px;
  background: white;
  border: 2px solid #e2e8f0;
  border-radius: 50px;
  font-weight: 600;
  font-size: 15px;
  color: #475569;
  cursor: pointer;
  transition: all 0.3s ease;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.02);
}

.category-tab:hover {
  border-color: #f97316;
  color: #f97316;
  transform: translateY(-2px);
  box-shadow: 0 10px 20px rgba(249, 115, 22, 0.15);
}

.category-tab.active {
  background: #f97316;
  border-color: #f97316;
  color: white;
  box-shadow: 0 10px 20px rgba(249, 115, 22, 0.3);
}

/* Courses Grid */
.courses-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 30px;
  margin-bottom: 50px;
}

/* Course Card */
.course-card {
  background: white;
  border-radius: 30px;
  overflow: hidden;
  box-shadow: 0 20px 40px -10px rgba(0, 0, 0, 0.1);
  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  position: relative;
  animation: fadeInUp 0.6s ease forwards;
  opacity: 0;
}

.course-card:nth-child(1) { animation-delay: 0.3s; }
.course-card:nth-child(2) { animation-delay: 0.4s; }
.course-card:nth-child(3) { animation-delay: 0.5s; }
.course-card:nth-child(4) { animation-delay: 0.6s; }
.course-card:nth-child(5) { animation-delay: 0.7s; }
.course-card:nth-child(6) { animation-delay: 0.8s; }
.course-card:nth-child(7) { animation-delay: 0.9s; }
.course-card:nth-child(8) { animation-delay: 1s; }
.course-card:nth-child(9) { animation-delay: 1.1s; }

.course-card:hover {
  transform: translateY(-15px) scale(1.02);
  box-shadow: 0 30px 60px -15px rgba(249, 115, 22, 0.3);
}

/* Card Image */
.card-image-wrapper {
  position: relative;
  height: 200px;
  overflow: hidden;
}

.course-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.6s ease;
}

.course-card:hover .course-image {
  transform: scale(1.1);
}

.image-overlay {
  position: absolute;
  inset: 0;
  background: linear-gradient(180deg, transparent 0%, rgba(0, 0, 0, 0.5) 100%);
}

/* Course Badge */
.course-badge {
  position: absolute;
  top: 15px;
  right: 15px;
  background: #f97316;
  color: white;
  padding: 6px 15px;
  border-radius: 50px;
  font-size: 12px;
  font-weight: 600;
  letter-spacing: 0.5px;
  z-index: 2;
  box-shadow: 0 5px 10px rgba(249, 115, 22, 0.3);
}

/* Card Content */
.card-content {
  padding: 25px;
}

.course-title {
  font-size: 22px;
  font-weight: 700;
  color: #0a1929;
  margin-bottom: 10px;
  transition: color 0.3s ease;
}

.course-card:hover .course-title {
  color: #f97316;
}

.course-meta {
  display: flex;
  gap: 20px;
  margin-bottom: 15px;
  font-size: 13px;
  color: #64748b;
}

.meta-item {
  display: flex;
  align-items: center;
  gap: 5px;
}

.meta-item svg {
  width: 14px;
  height: 14px;
  color: #f97316;
}

/* Tools List */
.tools-list {
  display: flex;
  flex-wrap: wrap;
  gap: 10px;
  margin: 20px 0;
}

.tool-tag {
  background: #f1f5f9;
  color: #475569;
  padding: 6px 15px;
  border-radius: 50px;
  font-size: 12px;
  font-weight: 500;
  transition: all 0.3s ease;
  border: 1px solid transparent;
}

.course-card:hover .tool-tag {
  background: rgba(249, 115, 22, 0.1);
  color: #f97316;
  border-color: rgba(249, 115, 22, 0.2);
}

/* Card Footer */
.card-footer {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20px;
  padding-top: 20px;
  border-top: 1px solid #e2e8f0;
}

.course-price {
  font-size: 24px;
  font-weight: 800;
  color: #f97316;
}

.course-price span {
  font-size: 14px;
  font-weight: 400;
  color: #94a3b8;
  margin-left: 5px;
}

.enroll-link {
  display: flex;
  align-items: center;
  gap: 8px;
  color: #64748b;
  text-decoration: none;
  font-weight: 600;
  font-size: 14px;
  transition: all 0.3s ease;
}

.enroll-link svg {
  transition: transform 0.3s ease;
}

.enroll-link:hover {
  color: #f97316;
}

.enroll-link:hover svg {
  transform: translateX(5px);
}

/* View All Button */
.view-all-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  margin: 0 auto;
  background: transparent;
  color: #0a1929;
  border: 2px solid #f97316;
  padding: 16px 40px;
  border-radius: 50px;
  font-weight: 700;
  font-size: 16px;
  cursor: pointer;
  transition: all 0.3s ease;
  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.05);
}

.view-all-btn:hover {
  background: #f97316;
  color: white;
  transform: translateY(-2px);
  box-shadow: 0 15px 30px rgba(249, 115, 22, 0.3);
}

.view-all-btn svg {
  transition: transform 0.3s ease;
}

.view-all-btn:hover svg {
  transform: translateX(5px);
}

/* Animations */
@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

/* Responsive Design */
@media (max-width: 1200px) {
  .courses-grid {
    grid-template-columns: repeat(2, 1fr);
  }
}

@media (max-width: 992px) {
  .popular-courses {
    padding: 80px 0;
  }

  .section-title {
    font-size: 40px;
  }
}

@media (max-width: 768px) {
  .courses-grid {
    grid-template-columns: 1fr;
    max-width: 450px;
    margin-left: auto;
    margin-right: auto;
  }

  .section-title {
    font-size: 32px;
  }

  .section-subtitle {
    font-size: 16px;
    padding: 0 20px;
  }

  .category-tab {
    padding: 10px 20px;
    font-size: 14px;
  }

  .card-image-wrapper {
    height: 180px;
  }
}

@media (max-width: 480px) {
  .container {
    padding: 0 16px;
  }

  .section-badge {
    font-size: 12px;
    padding: 6px 16px;
  }

  .section-title {
    font-size: 28px;
  }

  .category-tabs {
    gap: 10px;
  }

  .category-tab {
    padding: 8px 16px;
    font-size: 13px;
  }

  .course-title {
    font-size: 20px;
  }

  .course-price {
    font-size: 20px;
  }

  .view-all-btn {
    width: 100%;
    padding: 14px 20px;
  }
}

/* Loading Animation */
@keyframes shimmer {
  0% {
    background-position: -1000px 0;
  }
  100% {
    background-position: 1000px 0;
  }
}

.loading {
  animation: shimmer 2s infinite;
  background: linear-gradient(to right, #f1f5f9 4%, #e2e8f0 25%, #f1f5f9 36%);
  background-size: 1000px 100%;
}

/*  Course End */

