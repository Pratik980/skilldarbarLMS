  position: absolute;
  inset: 0;
  background-image: 
    radial-gradient(circle at 20% 30%, rgba(249, 115, 22, 0.1) 0%, transparent 40%),
    radial-gradient(circle at 80% 70%, rgba(249, 115, 22, 0.1) 0%, transparent 40%),
    repeating-linear-gradient(45deg, rgba(255,255,255,0.02) 0px, rgba(255,255,255,0.02) 2px, transparent 2px, transparent 10px);
  pointer-events: none;
}

/* Floating elements */
.floating-shape {
  position: absolute;
  width: 200px;
  height: 200px;
  border: 2px solid rgba(249, 115, 22, 0.1);
  border-radius: 30% 70% 70% 30% / 30% 30% 70% 70%;
  animation: floatShape 15s ease-in-out infinite;
}

.shape-1 {
  top: -50px;
  right: -50px;
  width: 300px;
  height: 300px;
  border-color: rgba(249, 115, 22, 0.1);
  animation-delay: 0s;
}

.shape-2 {
  bottom: -80px;
  left: -80px;
  width: 400px;
  height: 400px;
  border-color: rgba(249, 115, 22, 0.05);
  animation-delay: -5s;
}

.container {
  max-width: 1400px;
  margin: 0 auto;
  padding: 0 24px;
  position: relative;
  z-index: 2;
}

/* Section Header */
.section-header {
  text-align: center;
  margin-bottom: 70px;
  position: relative;
}

.section-badge {
  display: inline-flex;
  align-items: center;
  gap: 8px;
  background: rgba(249, 115, 22, 0.2);
  color: #f97316;
  font-weight: 600;
  font-size: 14px;
  padding: 8px 24px;
  border-radius: 50px;
  margin-bottom: 20px;
  text-transform: uppercase;
  letter-spacing: 1px;
  border: 1px solid rgba(249, 115, 22, 0.3);
  backdrop-filter: blur(10px);
  animation: fadeInUp 0.6s ease;
}

.section-badge svg {
  width: 18px;
  height: 18px;
}

.section-title {
  font-size: clamp(36px, 5vw, 48px);
  font-weight: 800;
  color: white;
  margin-bottom: 20px;
  animation: fadeInUp 0.6s ease 0.1s both;
  line-height: 1.2;
}

.section-title .highlight {
  color: #f97316;
  position: relative;
  display: inline-block;
}

.section-title .highlight::after {
  content: '';
  position: absolute;
  bottom: 5px;
  left: 0;
  width: 100%;
  height: 8px;
  background: rgba(249, 115, 22, 0.3);
  border-radius: 4px;
  z-index: -1;
}

.section-subtitle {
  font-size: 18px;
  color: rgba(255, 255, 255, 0.7);
  max-width: 600px;
  margin: 0 auto;
  animation: fadeInUp 0.6s ease 0.2s both;
  line-height: 1.6;
}

/* Process Steps */
.process-steps {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 30px;
  margin: 60px 0;
  position: relative;
}

/* Connecting line */
.process-steps::before {
  content: '';
  position: absolute;
  top: 100px;
  left: 15%;
  right: 15%;
  height: 3px;
  background: linear-gradient(90deg, 
    rgba(249, 115, 22, 0.1) 0%,
    rgba(249, 115, 22, 0.5) 50%,
    rgba(249, 115, 22, 0.1) 100%
  );
  transform: translateY(-50%);
  z-index: 1;
}

/* Step Card */
.step-card {
  background: rgba(255, 255, 255, 0.03);
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.05);
  border-radius: 30px;
  padding: 40px 30px;
  text-align: center;
  position: relative;
  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  z-index: 2;
  animation: fadeInUp 0.6s ease forwards;
  opacity: 0;
}

.step-card:nth-child(1) { animation-delay: 0.2s; }
.step-card:nth-child(2) { animation-delay: 0.3s; }
.step-card:nth-child(3) { animation-delay: 0.4s; }
.step-card:nth-child(4) { animation-delay: 0.5s; }

.step-card:hover {
  transform: translateY(-15px);
  border-color: rgba(249, 115, 22, 0.3);
  background: rgba(255, 255, 255, 0.05);
  box-shadow: 0 30px 50px -20px rgba(249, 115, 22, 0.3);
}

/* Step Number */
.step-number {
  position: absolute;
  top: -15px;
  left: 50%;
  transform: translateX(-50%);
  width: 40px;
  height: 40px;
  background: #f97316;
  color: white;
  font-weight: 800;
  font-size: 20px;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 5px 15px rgba(249, 115, 22, 0.4);
  z-index: 3;
  transition: all 0.3s ease;
}

.step-card:hover .step-number {
  transform: translateX(-50%) scale(1.1);
  background: white;
  color: #f97316;
  box-shadow: 0 8px 25px rgba(249, 115, 22, 0.5);
}

/* Step Icon */
.step-icon-wrapper {
  width: 100px;
  height: 100px;
  background: rgba(249, 115, 22, 0.1);
  border-radius: 30px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 20px auto 25px;
  transition: all 0.3s ease;
  position: relative;
}

.step-card:hover .step-icon-wrapper {
  background: #f97316;
  transform: scale(1.1) rotate(5deg);
  border-radius: 25px;
}

.step-icon {
  width: 50px;
  height: 50px;
  color: #f97316;
  transition: all 0.3s ease;
}

.step-card:hover .step-icon {
  color: white;
  transform: scale(1.1);
}

/* Step Content */
.step-title {
  font-size: 22px;
  font-weight: 700;
  color: white;
  margin-bottom: 15px;
  transition: color 0.3s ease;
}

.step-card:hover .step-title {
  color: #f97316;
}

.step-description {
  font-size: 15px;
  line-height: 1.7;
  color: rgba(255, 255, 255, 0.6);
  margin-bottom: 20px;
  transition: all 0.3s ease;
}

.step-card:hover .step-description {
  color: rgba(255, 255, 255, 0.8);
}

/* Step Features */
.step-features {
  list-style: none;
  padding: 0;
  margin: 0;
  text-align: left;
}

.step-features li {
  display: flex;
  align-items: center;
  gap: 10px;
  color: rgba(255, 255, 255, 0.5);
  font-size: 13px;
  margin-bottom: 10px;
  padding: 8px 12px;
  background: rgba(255, 255, 255, 0.02);
  border-radius: 8px;
  transition: all 0.3s ease;
}

.step-card:hover .step-features li {
  background: rgba(249, 115, 22, 0.1);
  color: rgba(255, 255, 255, 0.8);
  transform: translateX(5px);
}

.step-features li svg {
  width: 14px;
  height: 14px;
  color: #f97316;
  opacity: 0;
  transform: translateX(-10px);
  transition: all 0.3s ease;
}

.step-card:hover .step-features li svg {
  opacity: 1;
  transform: translateX(0);
}

.step-features li:nth-child(1) svg { transition-delay: 0s; }
.step-features li:nth-child(2) svg { transition-delay: 0.1s; }
.step-features li:nth-child(3) svg { transition-delay: 0.2s; }

/* Progress Stats */
.progress-stats {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 30px;
  margin-top: 70px;
  padding: 40px;
  background: rgba(255, 255, 255, 0.02);
  border-radius: 40px;
  border: 1px solid rgba(255, 255, 255, 0.05);
  backdrop-filter: blur(10px);
  animation: fadeInUp 0.6s ease 0.6s both;
}

.stat-item {
  text-align: center;
  position: relative;
}

.stat-item:not(:last-child)::after {
  content: '';
  position: absolute;
  top: 50%;
  right: -15px;
  transform: translateY(-50%);
  width: 1px;
  height: 40px;
  background: rgba(255, 255, 255, 0.1);
}

.stat-value {
  font-size: 36px;
  font-weight: 800;
  color: #f97316;
  margin-bottom: 5px;
  line-height: 1;
}

.stat-label {
  font-size: 14px;
  color: rgba(255, 255, 255, 0.6);
  text-transform: uppercase;
  letter-spacing: 1px;
}

.stat-desc {
  font-size: 12px;
  color: rgba(255, 255, 255, 0.4);
  margin-top: 5px;
}

/* Animations */
@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes floatShape {
  0%, 100% {
    transform: rotate(0deg) scale(1);
  }
  33% {
    transform: rotate(5deg) scale(1.1);
  }
  66% {
    transform: rotate(-5deg) scale(0.9);
  }
}

/* Responsive Design */
@media (max-width: 1200px) {
  .process-steps {
    grid-template-columns: repeat(2, 1fr);
    gap: 30px;
  }

  .process-steps::before {
    display: none;
  }

  .progress-stats {
    grid-template-columns: repeat(2, 1fr);
  }

  .stat-item:not(:last-child)::after {
    display: none;
  }
}

@media (max-width: 992px) {
  .learning-process {
    padding: 80px 0;
  }

  .section-title {
    font-size: 40px;
  }
}

@media (max-width: 768px) {
  .process-steps {
    grid-template-columns: 1fr;
    max-width: 400px;
    margin-left: auto;
    margin-right: auto;
  }

  .section-title {
    font-size: 32px;
  }

  .section-subtitle {
    font-size: 16px;
    padding: 0 20px;
  }

  .step-card {
    padding: 35px 25px;
  }

  .step-icon-wrapper {
    width: 80px;
    height: 80px;
  }

  .step-icon {
    width: 40px;
    height: 40px;
  }

  .step-title {
    font-size: 20px;
  }

  .progress-stats {
    grid-template-columns: 1fr;
    gap: 20px;
    padding: 30px 20px;
  }

  .floating-shape {
    display: none;
  }
}

@media (max-width: 480px) {
  .container {
    padding: 0 16px;
  }

  .section-badge {
    font-size: 12px;
    padding: 6px 16px;
  }

  .section-title {
    font-size: 28px;
  }

  .step-number {
    width: 35px;
    height: 35px;
    font-size: 18px;
  }

  .stat-value {
    font-size: 28px;
  }

  .stat-label {
    font-size: 12px;
  }
}

/* Animation for steps on scroll */
@keyframes stepPulse {
  0%, 100% {
    box-shadow: 0 0 0 0 rgba(249, 115, 22, 0.4);
  }
  50% {
    box-shadow: 0 0 0 20px rgba(249, 115, 22, 0);
  }
}

.step-card.active {
  animation: stepPulse 2s infinite;
}

/* Interactive hover effects */
.step-card {
  cursor: pointer;
}

.step-card:hover .step-features li {
  transform: translateX(10px);
}

/* Learning process End */

