.courses-section {
  font-family: 'Inter', sans-serif;
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  padding: 100px 0;
  position: relative;
  overflow: hidden;
}

/* Background decoration */
.courses-section::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: url('https://images.unsplash.com/photo-1557804506-669a67965ba0?ixlib=rb-4.0.3&auto=format&fit=crop&w=1974&q=80') center/cover;
  opacity: 0.1;
  pointer-events: none;
}

.courses-container {
  max-width: 1400px;
  margin: 0 auto;
  padding: 0 40px;
  position: relative;
  z-index: 1;
}

/* Section Header */
.section-header {
  text-align: center;
  margin-bottom: 70px;
}

.section-subtitle {
  display: inline-block;
  font-size: 18px;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 2px;
  color: #ffd700;
  background: rgba(255, 255, 255, 0.15);
  padding: 8px 20px;
  border-radius: 30px;
  margin-bottom: 20px;
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.2);
}

.section-title {
  font-size: 52px;
  font-weight: 800;
  color: #ffffff;
  margin-bottom: 25px;
  line-height: 1.2;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.2);
}

.highlight {
  color: #ffd700;
  position: relative;
  display: inline-block;
}

.highlight::after {
  content: '';
  position: absolute;
  bottom: 5px;
  left: 0;
  width: 100%;
  height: 8px;
  background: rgba(255, 215, 0, 0.3);
  z-index: -1;
}

.section-description {
  font-size: 20px;
  color: rgba(255, 255, 255, 0.9);
  max-width: 700px;
  margin: 0 auto;
  line-height: 1.6;
}

/* Courses Grid */
.courses-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 35px;
  margin-bottom: 60px;
}

/* Course Card */
.course-card {
  background: rgba(255, 255, 255, 0.95);
  backdrop-filter: blur(10px);
  border-radius: 30px;
  overflow: hidden;
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.2);
  transition: all 0.4s cubic-bezier(0.165, 0.84, 0.44, 1);
  border: 1px solid rgba(255, 255, 255, 0.3);
  position: relative;
}

.course-card:hover {
  transform: translateY(-15px) scale(1.02);
  box-shadow: 0 40px 60px rgba(0, 0, 0, 0.3);
  background: #ffffff;
}

/* Course Image */
.course-image-wrapper {
  position: relative;
  height: 280px;
  overflow: hidden;
}

.course-image {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.6s ease;
}

.course-card:hover .course-image {
  transform: scale(1.1);
}

.course-level-badge {
  position: absolute;
  top: 20px;
  right: 20px;
  background: #ffd700;
  color: #2d3748;
  padding: 8px 16px;
  border-radius: 25px;
  font-size: 14px;
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: 1px;
  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);
  z-index: 2;
}

/* Course Content */
.course-content {
  padding: 30px;
}

.course-title {
  font-size: 28px;
  font-weight: 700;
  color: #2d3748;
  margin-bottom: 12px;
  line-height: 1.3;
}

.course-description {
  font-size: 16px;
  color: #718096;
  margin-bottom: 25px;
  line-height: 1.6;
}

.course-details {
  display: flex;
  gap: 25px;
  margin-bottom: 25px;
  padding-bottom: 25px;
  border-bottom: 2px solid #edf2f7;
}

.detail-item {
  display: flex;
  align-items: center;
  gap: 8px;
  font-size: 16px;
  color: #4a5568;
  font-weight: 500;
}

.detail-icon {
  width: 20px;
  height: 20px;
  color: #667eea;
}

/* View Details Button */
.view-details-btn {
  width: 100%;
  padding: 16px;
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  color: white;
  border: none;
  border-radius: 15px;
  font-size: 18px;
  font-weight: 600;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 10px;
  transition: all 0.3s ease;
  position: relative;
  overflow: hidden;
}

.view-details-btn::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
  transition: left 0.5s ease;
}

.view-details-btn:hover::before {
  left: 100%;
}

.view-details-btn:hover {
  background: linear-gradient(135deg, #764ba2 0%, #667eea 100%);
  transform: scale(1.05);
  box-shadow: 0 10px 20px rgba(102, 126, 234, 0.4);
}

.btn-icon {
  width: 20px;
  height: 20px;
  transition: transform 0.3s ease;
}

.view-details-btn:hover .btn-icon {
  transform: translateX(5px);
}

/* View More Section */
.view-more-wrapper {
  text-align: center;
}

.view-more-btn {
  background: transparent;
  color: white;
  border: 2px solid #ffd700;
  padding: 18px 45px;
  font-size: 20px;
  font-weight: 700;
  border-radius: 50px;
  cursor: pointer;
  display: inline-flex;
  align-items: center;
  gap: 12px;
  transition: all 0.3s ease;
  margin-bottom: 20px;
  text-transform: uppercase;
  letter-spacing: 1px;
}

.view-more-btn:hover {
  background: #ffd700;
  color: #2d3748;
  transform: translateY(-3px);
  box-shadow: 0 20px 30px rgba(255, 215, 0, 0.3);
}

.view-more-btn:hover .btn-icon {
  stroke: #2d3748;
}

.login-prompt {
  color: rgba(255, 255, 255, 0.8);
  font-size: 16px;
  font-weight: 400;
  letter-spacing: 0.5px;
}

/* Responsive Design */
@media screen and (max-width: 1200px) {
  .courses-grid {
    gap: 25px;
  }
  
  .section-title {
    font-size: 44px;
  }
}

@media screen and (max-width: 992px) {
  .courses-grid {
    grid-template-columns: repeat(2, 1fr);
  }
  
  .course-title {
    font-size: 24px;
  }
}

@media screen and (max-width: 768px) {
  .courses-section {
    padding: 70px 0;
  }
  
  .courses-container {
    padding: 0 20px;
  }
  
  .section-title {
    font-size: 36px;
  }
  
  .section-description {
    font-size: 18px;
  }
  
  .course-image-wrapper {
    height: 240px;
  }
  
  .course-details {
    flex-direction: column;
    gap: 15px;
  }
}

@media screen and (max-width: 576px) {
  .courses-grid {
    grid-template-columns: 1fr;
  }
  
  .section-title {
    font-size: 30px;
  }
  
  .course-card {
    max-width: 400px;
    margin: 0 auto;
  }
  
  .view-more-btn {
    padding: 15px 35px;
    font-size: 18px;
  }
}

/* Animation Keyframes */
@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

.course-card {
  animation: fadeInUp 0.6s ease forwards;
}

.course-card:nth-child(1) { animation-delay: 0.1s; }
.course-card:nth-child(2) { animation-delay: 0.2s; }
.course-card:nth-child(3) { animation-delay: 0.3s; }
.course-card:nth-child(4) { animation-delay: 0.4s; }





